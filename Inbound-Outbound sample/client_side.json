{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "yacd",
            "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
            "external_ui_download_detour": "direct",
            "default_mode": "rule"
        },
        "cache_file": {
            "enabled": true,
            "store_fakeip": false
        }
    },
    "dns": {
        "servers": [
            {
                "address": "https://1.1.1.1/dns-query",
                "address_resolver": "dns-local",
                "detour": "proxy",
                "strategy": "prefer_ipv4",
                "tag": "dns-remote"
            },
            {
                "address": "tcp://8.8.8.8",
                "address_resolver": "dns-local",
                "detour": "direct",
                "strategy": "prefer_ipv4",
                "tag": "dns-direct"
            },
            {
                "address": "rcode://success",
                "tag": "dns-block"
            },
            {
                "address": "local",
                "detour": "direct",
                "tag": "dns-local"
            },
            {
                "tag": "dns-fakeip",
                "address": "fakeip",
                "strategy": "prefer_ipv4"
            }
        ],
        "rules": [
            {
                "geosite": [
                    "social"
                ],
                "server": "dns-remote"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "rule_set": [
                            "geoip-ir",
                            "geosite-ir"
                        ],
                        "domain_suffix": [
                            ".ir"
                        ]
                    }
                ],
                "server": "dns-direct"
            },
            {
                "outbound": "direct",
                "server": "dns-direct",
                "rewrite_ttl": 20
            },
            {
                "outbound": "any",
                "server": "dns-direct",
                "rewrite_ttl": 20
            },
            {
                "query_type": [
                    32,
                    33
                ],
                "server": "dns-block"
            },
            {
                "domain_suffix": ".lan",
                "server": "dns-block"
            },
            {
                "domain_suffix": [
                    ".arpa",
                    ".arpa."
                ],
                "server": "dns-block"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "dns-fakeip"
            },
            {
                "disable_cache": true,
                "inbound": [
                    "tun-in"
                ],
                "server": "dns-fakeip"
            }
        ],
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "reverse_mapping": true,
        "disable_cache": false,
        "disable_expire": false,
        "strategy": "prefer_ipv4",
        "independent_cache": true,
        "final": "dns-remote"
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "domain_strategy": "prefer_ipv4",
            "interface_name": "tun0",
            "inet4_address": "172.19.0.1/30",
            "mtu": 1500,
            "auto_route": true,
            "strict_route": true,
            "stack": "gvisor",
            "endpoint_independent_nat": true,
            "sniff": true,
            "sniff_override_destination": false
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "type": "selector",
            "outbounds": [
                "URL-TEST",
                ""
                // configs name
            ]
        },
        {
            "tag": "URL-TEST",
            "type": "urltest",
            "outbounds": [
                ""
                // configs name
            ],
            "url": "https://detectportal.firefox.com/success.txt",
            "interval": "60s",
            "tolerance": 50
        },
        //  configs

        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "override_android_vpn": true,
        "final": "proxy",
        "rules": [
            {
                "outbound": "dns-out",
                "protocol": "dns"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "network": "udp",
                "outbound": "block",
                "port": [
                    135,
                    137,
                    138,
                    139,
                    5353
                ]
            },
            {
                "domain_suffix": [
                    ".ir"
                ],
                "outbound": "direct"
            },
            {
                "network": "quic",
                "port": 443,
                "outbound": "block"
            },
            {
                "ip_cidr": [
                    "224.0.0.0\/3",
                    "ff00::\/8"
                ],
                "outbound": "block",
                "source_ip_cidr": [
                    "224.0.0.0\/3",
                    "ff00::\/8"
                ]
            },
            {
                "domain_suffix": [
                    "appcenter.ms",
                    "firebase.io",
                    "crashlytics.com"
                ],
                "outbound": "block"
            },
            {
                "rule_set": [
                    "geosite-category-bank-ir",
                    "geosite-category-bourse-ir",
                    "geosite-category-education-ir",
                    "geosite-category-forums-ir",
                    "geosite-category-gov-ir",
                    "geosite-category-insurance-ir",
                    "geosite-category-ir",
                    "geosite-category-media-ir",
                    "geosite-category-payment-ir",
                    "geosite-category-shopping-ir",
                    "geosite-category-tech-ir",
                    "geosite-category-travel-ir",
                    "geosite-cn"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "geosite-category-ads-all",
                    "geosite-malware",
                    "geosite-phishing",
                    "geosite-cryptominers",
                    "geoip-malware",
                    "geoip-phishing",
                    "geosite-category-public-tracker",
                    "geosite-category-ads-ir"
                ],
                "outbound": "block"
            },
            {
                "rule_set": [
                    "geosite-ir",
                    "geoip-ir",
                    "geoip-iranserver",
                    "geoip-ir-asiatech",
                    "geoip-ru",
                    "geoip-cn"
                ],
                "outbound": "direct"
            }
        ],
