{
    "log": {
        "disabled": false,
        "level": "info",
        "output": "/etc/sing-box/box.log",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "dns-remote",
                "address": "https://dns.nextdns.io/184596",
                "detour": "direct",
                "address_resolver": "dns-local",
                "address_strategy": "prefer_ipv4"
            },
            {
                "address": "local",
                "detour": "direct",
                "tag": "dns-local"
            },
            {
                "tag": "block",
                "address": "rcode://success"
            }
        ],
        "rules": [
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": [
                            "geoip-ir",
                            "geosite-ir"
                        ],
                        "domain_suffix": [
                            ".ir"
                        ]
                    }
                ],
                "server": "dns-local"
            },
            {
                "outbound": "block",
                "server": "dns-local",
                "rewrite_ttl": 20
            },
            {
                "outbound": "any",
                "server": "dns-remote",
                "rewrite_ttl": 20
            }
        ],
        "strategy": "prefer_ipv4",
        "final": "dns-remote",
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": false,
        "reverse_mapping": true
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "",
            "cache_id": "",
            "store_fakeip": false
        }
    },
    "inbounds": [

// add your inbound (vless, vmess, etc)

    ],
    "outbounds": [
        // wireguard outbound
        {
            "server": "engage.cloudflareclient.com",
            "server_port": 2408,
            "local_address": [
                "172.16.0.2/32",
                "2606:4700:110:81d0:a1dc:7e2d:b127:b381/128"
            ],
            "private_key": "",
            "peer_public_key": "",
            "reserved": [
                0,
                0,
                0
            ],
            "tag": "wg",
            "type": "wireguard"
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "rules": [
            {
                "ip_is_private": true,
                "outbound": "block"
            },
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "network": "udp",
                        "port_range": [
                            "1024:65534"
                        ]
                    },
                    {
                        "network": "quic",
                        "port": 443
                    },
                    {
                        "source_ip_cidr": [
                            "224.0.0.0\/3",
                            "ff00::\/8"
                        ]
                    },
                    {
                        "ip_cidr": [
                            "224.0.0.0\/3",
                            "ff00::\/8"
                        ]
                    }
                ],
                "outbound": "block"
            },
            {
                "domain_suffix": [
                    ".ir"
                ],
                "outbound": "block"
            },
            {
                "rule_set": [
                    "geosite-category-ads-all",
                    "geosite-malware",
                    "geosite-phishing",
                    "geosite-cryptominers",
                    "geoip-malware",
                    "geoip-phishing",
                    "geosite-category-public-tracker",
                    "geosite-category-ads-ir",
                    "geosite-category-bank-ir",
                    "geosite-category-bourse-ir",
                    "geosite-category-education-ir",
                    "geosite-category-forums-ir",
                    "geosite-category-gov-ir",
                    "geosite-category-insurance-ir",
                    "geosite-category-ir",
                    "geosite-category-media-ir",
                    "geosite-category-payment-ir",
                    "geosite-category-shopping-ir",
                    "geosite-category-tech-ir",
                    "geosite-category-travel-ir",
                    "geosite-cn"
                ],
                "outbound": "block"
            },
            {
                "rule_set": [
                    "geosite-ir",
                    "geoip-ir",
                    "geoip-iranserver",
                    "geoip-ir-asiatech",
                    "geoip-ru",
                    "geoip-cn"
                ],
                "outbound": "block"
            }
